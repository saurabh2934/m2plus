public class solution{
     public static bolean findArray(int arr[],int element,int index){
	if(index==arr.lenght){
		return false;
	}
        if(arr[index]==element){
		return true;
		}
          else{
      	     return  findArray(arr,element,index+1);
	}
}
 public staic void main(String[]args){
    int arr[]={1,2,5,21,89};
    System.out.println(findArray(arr,5,0);
}
